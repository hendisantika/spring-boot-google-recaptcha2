# Google reCAPTCHA v3 Configuration
# Copy this file to .env and fill in your actual keys
# Get your keys from: https://www.google.com/recaptcha/admin

# reCAPTCHA v3 Secret Key (server-side)
RECAPTCHA_SECRET_KEY=your_secret_key_here

# reCAPTCHA v3 Site Key (client-side)
RECAPTCHA_SITE_KEY=your_site_key_here

# Score threshold (optional, default: 0.5)
# Higher values = stricter validation
# Range: 0.0 (all bots) to 1.0 (all humans)
RECAPTCHA_THRESHOLD=0.5

# Database Configuration (optional - override defaults)
#DB_URL=jdbc:mysql://localhost:3306/capcay
#DB_USERNAME=root
#DB_PASSWORD=root
